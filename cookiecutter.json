{
            "name": "package",
         "version": "0.1.0",
            "slug": "{{ cookiecutter.name.lower().replace('-', '_').replace(' ', '_').replace('.', '') }}",
         "command": "{{ cookiecutter.name.lower().replace(' ', '-') }}",
             "cli": ["click", "argparse", "none"],
          "compat": ["upyog", "none"],
         "jupyter": "n",
          "sqlite": "n",
            "pypy": "y",
     "description": "",
          "author": "",
           "email": "",
        "keywords": "{{ cookiecutter.description.lower() }}",
         "license": ["MIT License", "Apache License 2.0", "GNU General Public License v3.0", "none"],
    "repo_service": ["https://github.com", "https://gitlab.com"],
    "repo_service_username": "",
    "repo_service_reponame": "{{ cookiecutter.name.lower().replace(' ', '-') }}",
    "docker_registry": "ghcr.io",
    "docker_service_username": "{{ cookiecutter.repo_service_username }}",
    "docker_service_reponame": "{{ cookiecutter.repo_service_reponame }}",
            "github_action": "n",
                       "ml": "n",
                      "api": "n",
                   "editor": ["vscode", "other"],
                      "url": "{{ cookiecutter.repo_service }}/{{ cookiecutter.repo_service_username }}/{{ cookiecutter.repo_service_reponame }}",
              "singularity": "n",
                      "ros": "n",
   "_copy_without_render": [
      ".github/workflows/ci.yml"
   ]
}